{
	"compilerOptions": {
		"allowImportingTsExtensions":true,
		"jsx": "react",
		"jsxFactory": "window.RUMIOUS_JSX.createElement",
		"declaration": true,
		"declarationDir": "./dist/",
		"outDir": "./dist",
		"esModuleInterop": true,
		"strict": true,
		"target": "ES6",
		"module": "NodeNext",
		"moduleResolution":"nodenext"
	},
	"include": ["src/**/*", "**/jsx.d.ts"],
	"exclude": ["node_modules"]
}