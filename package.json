{
	"type":"module",
	"scripts": {
		"dec": "tsc --declaration --allowJs --emitDeclarationOnly",
		"type": "tsc --noEmit",
		"build": "rumious build prod",
		"build:dev": "rumious build dev ",
		"dev": "rumious build dev -w",
		"style": "esbuild src/styles/main.css --bundle --outfile=public/dist/styles/index.css --loader:.css=css --loader:.ttf=file --loader:.otf=file --minify",
		"start": "node dist/index.js",
		"build:all":"export NODE_ENV='production' && rollup -c --bundleConfigAsCjs && npm run style",
		"test": "echo \"No tests yet\""
	},
	"dependencies": {
		"lightiz-ui": "^0.0.3",
		"rumious": "*",
		"rumious-router": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.26.10",
		"@babel/plugin-syntax-jsx": "^7.25.9",
		"@babel/preset-typescript": "^7.27.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^28.0.3",
		"@rollup/plugin-node-resolve": "^16.0.1",
		"@rollup/plugin-terser": "^0.4.4",
		"babel-plugin-rumious": "*",
		"esbuild": "*",
		"rollup": "^4.39.0",
		"tslib": "^2.8.1",
		"typescript": "^4.5.0"
	}
}